package view
{
	import flash.display.Sprite;
	
	import model.ClockData;
	
	public class AbstractClockView extends Sprite
	{
		//=========================================================
		// VARIABLES
		//=========================================================
		protected var _data:ClockData;
		
		//=========================================================
		// PUBLIC METHODS
		//=========================================================
		/**
		 * constructor 
		 * @param data
		 * 
		 */		
		public function AbstractClockView(data:ClockData)
		{
			super();
			_data = data;
			
		}
	}
}